<%= form_for(@pessoafisica) do |f| %>
  <% if @pessoafisica.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@pessoafisica.errors.count, "error") %> prohibited this pessoafisica from being saved:</h2>

      <ul>
      <% @pessoafisica.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nome %><br>
    <%= f.text_field :nome %>
  </div>
  <div class="field">
    <%= f.label :datanascimento %><br>
    <%= f.text_field :datanascimento %>
  </div>
  <div class="field">
    <%= f.label :endereco %><br>
    <%= f.text_field :endereco %>
  </div>
  <div class="field">
    <%= f.label :numero %><br>
    <%= f.text_field :numero %>
  </div>
  <div class="field">
    <%= f.label :complemento %><br>
    <%= f.text_field :complemento %>
  </div>
  <div class="field">
    <%= f.label :bairro %><br>
    <%= f.text_field :bairro %>
  </div>
  <div class="field">
    <%= f.label :cidade %><br>
    <%= f.text_field :cidade %>
  </div>
  <div class="field">
    <%= f.label :estado %><br>
    <%= f.text_field :estado %>
  </div>
  <div class="field">
    <%= f.label :telefone %><br>
    <%= f.text_field :telefone %>
  </div>
  <div class="field">
    <%= f.label :cpf %><br>
    <%= f.text_field :cpf %>
  </div>
  <div class="field">
    <%= f.label :contacorrente_id %><br>
    <%= f.text_field :contacorrente_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
